{
  "name": "hono-typed-rest",
  "version": "0.3.1",
  "description": "A library for building type-safe REST clients from Hono's application types (`AppType`).",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check": "tsc --noEmit"
  },
  "keywords": [
    "hono",
    "rest",
    "client",
    "typed"
  ],
  "author": "andy",
  "license": "MIT",
  "peerDependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "hono": "^4.0.0"
  }
}

